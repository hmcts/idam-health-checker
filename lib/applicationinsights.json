{
  "connectionString": "${APP_INSIGHTS_CONNECTION_STRING}",
  "role": {
    "name": "idam-health-checker"
  },
  "preview": {
    "roleNameOverrides": [
      {
        "httpPathPrefix": "/health",
        "roleName": "idam-health-checker (health)"
      }
    ],
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 100
        }
      ]
    }
  },
  "instrumentation": {
    "micrometer": {
      "enabled": false
    }
  }
}
